package Daily_Practice_Coding;

import java.util.Arrays;

public class Daily_Practice_Question {

	public static void main(String[] args) {
		int a[] = { 1, 2, 4, 5, 6, 7 };

		int sumarray = 0;
		for (int i = 0; i < a.length; i++) {
			sumarray = sumarray + a[i];
		}
		int sumto = 0;
		for (int i = 1; i < =7; i++) {
			sumto = sumto + i;
		}
		System.out.println("Missing number is::" + (sumto - sumarray));
	}
}
