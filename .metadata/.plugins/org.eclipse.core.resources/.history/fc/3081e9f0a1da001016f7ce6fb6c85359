package Daily_Practice_Coding;

import java.util.Arrays;

public class Daily_Practice_Question {

	public static void main(String[] args) {
		int a[] = { 1, 2, 3, 4, 5 };
		int sum = 1;
		for (int i = 0; i < a.length; i++) {
			sum = sum + a[i];
		}
		System.out.println("Factorial Number is ::" + sum);
	}
}
