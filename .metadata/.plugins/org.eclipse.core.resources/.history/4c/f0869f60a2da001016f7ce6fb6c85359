package Daily_Practice_Coding;

import java.util.Arrays;

public class Daily_Practice_Question {

	public static void main(String[] args) {
		int a = 0;
		int b = 1;
		System.out.print(a + ",");
		System.out.println(b + ",");
		int c = 0;
		for (int i = 0; i < 10; i++) {
			c = a + b;
			a = b;
			b = c;
		}
		System.out.print(c + ",");

	}
}
