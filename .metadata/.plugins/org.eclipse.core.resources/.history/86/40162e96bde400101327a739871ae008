package Pattern;

public class Tringle {

	public static void main(String[] args) {

		for (int i = 0; i <6; i++)

		{
           for(int j=0;j<6-i-1;j++)
           {
        	   System.out.print(" ");
           }
           for(int k=1;k<=(2*i+1);k++)
           {
        	   System.out.print("* ");
           }
           
           System.out.println();
		}
	}
}
